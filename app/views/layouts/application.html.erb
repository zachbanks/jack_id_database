<!DOCTYPE html>
<html>
<head>
  <title>JackIdDatabase</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	
	<!-- Render flash messages. -->
	<% flash.each do |name, msg| %>
		<%= content_tag(:div, raw(msg), :id => "flash_#{name}") %>
	<% end %>
	
	<%= yield %>

</body>
</html>
