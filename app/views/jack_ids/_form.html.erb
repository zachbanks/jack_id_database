<%= render :partial => 'shared/error_messages', :locals => { :object => @jack_id } %>
<%= form_for @jack_id do |f| %>
	<div class="field">
		<%= f.label :label %>
		<%= f.text_field :label %>
	</div>
	
	<div class="field">
		<%= f.label :connections %>
		<%= f.text_field :connections %>
	</div>
	
	<div class="field">
		<%= f.submit (@jack_id.new_record?) ? "Add Jack ID" : "Update Jack ID" %>
	</div>
<% end %>